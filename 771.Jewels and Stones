class Solution:
    def numJewelsInStones(self, J: str, S: str) -> int:
        #自己的
        # x = 0
        # for i in range(len(J)):
        #     for j in S:
        #         if J[i] == j:
        #             x += 1
        # return x
        
        #别人的
        return sum(ele in J for ele in S)
