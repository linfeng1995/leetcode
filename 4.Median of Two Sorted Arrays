class Solution:
    def findMedianSortedArrays(self, nums1: List[int], nums2: List[int]) -> float:
        nums1.extend(nums2)  #数组2加到数组1
        nums1.sort()   #简单的排序 从小到大
        lengh = len(nums1)
        if lengh == 0:  #是否为空
            result = None 
        else:
            a = lengh//2
            if lengh % 2 == 1:
                result = float(nums1[a])
            else:
                result = float((nums1[a-1]+nums1[a])/2)
        return result
